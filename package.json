{
  "name": "stockracer",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "npx expo start",
    "start:local": "EXPO_APP_ENV=local npx expo start",
    "start:prod": "EXPO_APP_ENV=prod npx expo start",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . && deno lint && yarn test",
    "test": "deno test --allow-all --env-file=./supabase/.env supabase/functions/tests/*",
    "typecheck": "tsc -b",
    "build": "echo ok",
    "sb:curl": "sh -c 'curl -sS -X POST ${SUPABASE_FUNCTION_URL_PREFIX:?set SUPABASE_FUNCTION_URL_PREFIX}/$npm_config_fn -H \"Content-Type: application/json\" -d \"${npm_config_data:-{}}\"'",
    "types": "supabase gen types typescript --local > ./supabase/functions/_shared/database.types.ts",
    "sb:deploy:fetch-symbols": "supabase functions deploy fetch-symbols"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "typescript": "^5.6.3"
  }
}
