{
  "name": "stockracer",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "npx expo start",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . && deno lint supabase/functions/tests/*",
    "test": "deno test --allow-all --env-file=./supabase/.env supabase/functions/tests/*",
    "typecheck": "tsc -b",
    "build": "echo ok",
    "sb:curl": "sh -c 'curl -sS -X POST ${SUPABASE_FUNCTION_URL_PREFIX:?set SUPABASE_FUNCTION_URL_PREFIX}/$npm_config_fn -H \"Content-Type: application/json\" -d \"${npm_config_data:-{}}\"'",
    "supabase:gen-types:local": "supabase gen types typescript --local --schema public,auth,storage   > database.types.ts",
    "sb:deploy:fetch-symbols": "supabase functions deploy fetch-symbols"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "typescript": "^5.6.3"
  }
}
